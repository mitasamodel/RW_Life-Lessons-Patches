<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/WorkGiverDef[defName="DoBillsMachiningTable"]/modExtensions/li[@Class="LifeLessons.BillProficiencyExtension" and not(requirements)]</xpath>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/WorkGiverDef[defName="DoBillsMachiningTable"]/modExtensions/li[@Class="LifeLessons.BillProficiencyExtension"]</xpath>
			<value>
				<requirements>
					<li>MachineOperator</li>
				</requirements>
				<unqualifiedPenalty>
					<statModifiers>
						<li>
							<stat>GeneralLaborSpeed</stat>
							<applyGlobally>True</applyGlobally>
							<modifier>0.3</modifier>
						</li>
					</statModifiers>
					<qualityCap>Normal</qualityCap>
				</unqualifiedPenalty>
				<hardRequirement>False</hardRequirement>
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/WorkGiverDef[defName="DoBillsMachiningTable"]/modExtensions/li[@Class="LifeLessons.BillProficiencyExtension"]</xpath>
		<nomatch Class="PatchOperationAddModExtension">
			<xpath>Defs/WorkGiverDef[defName="DoBillsMachiningTable"]</xpath>
			<value>
				<li Class="LifeLessons.BillProficiencyExtension">
					<requirements>
						<li>MachineOperator</li>
					</requirements>
					<rewards>
						<li>
							<proficiency>MachineOperator</proficiency>
							<experienceType>Practical</experienceType>
							<progressGain>0</progressGain>
							<progressMultiplier>1</progressMultiplier>
						</li>
					</rewards>
					<unqualifiedPenalty>
						<statModifiers>
							<li>
								<stat>GeneralLaborSpeed</stat>
								<applyGlobally>True</applyGlobally>
								<modifier>0.3</modifier>
							</li>
						</statModifiers>
						<qualityCap>Normal</qualityCap>
					</unqualifiedPenalty>
					<hardRequirement>False</hardRequirement>
				</li>
			</value>
		</nomatch>
	</Operation>
</Patch>